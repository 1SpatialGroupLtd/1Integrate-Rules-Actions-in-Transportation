                <Item id="RULE://Standard/Essential Geometric Checks/Check that Features are Topologically Structured">
                    <Metadata>
                        <Name>Check that Features are Topologically Structured</Name>
                        <Description>Tests whether an object is topologically structured. It returns a boolean value, true if the object is structured and false otherwise.</Description>
                        <Comments></Comments>
                        <Created>
                            <UserDate date="2017-01-03T01:13:12.000Z" user="1Spatial"/>
                        </Created>
                        <LastEdited>
                            <UserDate date="2018-10-03T04:15:07.000Z" user="1Spatial"/>
                        </LastEdited>
                    </Metadata>
                    <IsShortcut>false</IsShortcut>
                    <Rule>
                        <RootPredicate classLabel="" objLabel="all">
                            <RelationalPredicate>
                                <BuiltinFnValue fnName="is_structured">
                                    <DynamicValue classRef="" objRef="all" propName="geometry"/>
                                </BuiltinFnValue>
                                <EqualsRelation/>
                                <StaticValue datatype="boolean" value="true"/>
                            </RelationalPredicate>
                        </RootPredicate>
                    </Rule>
                </Item>
